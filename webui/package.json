{
  "name": "webui",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "serve": "vite preview",
    "refresh-api-schema": "docker run --rm -v $PWD:/local openapitools/openapi-generator-cli generate -g typescript-axios -i http://host.docker.internal:8000/openapi.json -o /local/src/api-client --additional-properties=supportsES6=true",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test-watch": "jest --watch"
  },
  "dependencies": {
    "@openapitools/openapi-generator-cli": "^2.2.3",
    "@vueuse/core": "^5.0.0",
    "axios": "^0.21.1",
    "color": "^3.1.3",
    "date-input-polyfill": "^2.14.0",
    "http-status-codes": "^2.1.4",
    "material-icons": "^0.7.0",
    "moment": "^2.29.1",
    "monaco-editor": "^0.24.0",
    "splitpanes": "^3.0.4",
    "vue": "^3.0.11",
    "vue-router": "^4.0.8",
    "vue3-markdown-it": "^1.0.8"
  },
  "devDependencies": {
    "@types/color": "^3.0.1",
    "@types/jest": "^26.0.22",
    "@vitejs/plugin-vue": "^1.2.3",
    "@vitejs/plugin-vue-jsx": "^1.1.5",
    "@vue/compiler-sfc": "^3.0.5",
    "jest": "^27.3.1",
    "sass": "^1.32.8",
    "ts-jest": "^27.0.7",
    "typescript": "^4.3.2",
    "vite": "^2.3.6",
    "vite-tsconfig-paths": "^3.3.13",
    "vue-tsc": "^0.0.26"
  }
}
