{
  "name": "webui",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "serve": "vite preview",
    "refresh-api-schema": "openapi-generator-cli generate -g typescript-axios -i http://localhost:8000/openapi.json -o ./src/api-client --additional-properties=supportsES6=true"
  },
  "dependencies": {
    "@openapitools/openapi-generator-cli": "^2.2.3",
    "axios": "^0.21.1",
    "http-status-codes": "^2.1.4",
    "vue": "^3.0.5",
    "vue-router": "^4.0.5"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^1.1.5",
    "@vitejs/plugin-vue-jsx": "^1.1.2",
    "@vue/compiler-sfc": "^3.0.5",
    "sass": "^1.32.8",
    "typescript": "^4.1.3",
    "vite": "^2.1.0",
    "vue-tsc": "^0.0.8"
  }
}
