{
  "name": "webui",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "serve": "vite preview",
    "refresh-api-schema": "openapi-generator-cli generate -g typescript-axios -i http://localhost:8000/openapi.json -o ./src/api-client --additional-properties=supportsES6=true",
    "type-check": "tsc --noEmit",
    "test": "jest"
  },
  "dependencies": {
    "@openapitools/openapi-generator-cli": "^2.2.3",
    "@vueuse/core": "^4.6.3",
    "axios": "^0.21.1",
    "color": "^3.1.3",
    "date-input-polyfill": "^2.14.0",
    "http-status-codes": "^2.1.4",
    "material-design-icons": "^3.0.1",
    "moment": "^2.29.1",
    "vue": "^3.0.5",
    "vue-router": "^4.0.5"
  },
  "devDependencies": {
    "@types/color": "^3.0.1",
    "@types/jest": "^26.0.22",
    "@vitejs/plugin-vue": "^1.1.5",
    "@vitejs/plugin-vue-jsx": "^1.1.2",
    "@vue/compiler-sfc": "^3.0.5",
    "jest": "^26.6.3",
    "sass": "^1.32.8",
    "ts-jest": "^26.5.5",
    "typescript": "^4.1.3",
    "vite": "^2.1.0",
    "vite-tsconfig-paths": "^2.3.0",
    "vue-tsc": "^0.0.8"
  }
}
