{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "scss:types": "typed-scss-modules src",
    "scss:watch": "typed-scss-modules src --watch",
    "generate-api": "cd ../backend && make openapi-for-frontend > ../frontend/openapi.json && cd ../frontend && npx @rtk-query/codegen-openapi openapi-config.cjs",
    "build:stellar-globe": "cd ../external/stellar-globe/stellar-globe && npm install && npm run build && cd ../react-stellar-globe && npm install && npm run build"
  },
  "dependencies": {
    "@floating-ui/react": "^0.27.16",
    "@reduxjs/toolkit": "^2.11.2",
    "@stellar-globe/react-stellar-globe": "file:../external/stellar-globe/react-stellar-globe",
    "@stellar-globe/stellar-globe": "file:../external/stellar-globe/stellar-globe",
    "material-symbols": "^0.40.2",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-hook-form": "^7.69.0",
    "react-redux": "^9.2.0",
    "react-router-dom": "^7.11.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@rtk-query/codegen-openapi": "^2.2.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "jsdom": "^27.4.0",
    "sass": "^1.97.1",
    "ts-node": "^10.9.2",
    "typed-scss-modules": "^8.1.1",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.4",
    "vite": "^7.2.4",
    "vitest": "^4.0.16"
  }
}
